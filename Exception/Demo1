class Demo1 extends Exception
{
    public static void main(String a[])
    {
        int n1 = 5;
        int n2 = 0;

        try
        {
            r = n1/n2;
            System.out.println(r);
        }
        catch(Exception e)
        { 
            System.out.println(e);
        }
    }
}
